<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.nativeaudio" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pocket Sampler</name>
    <description>
        Pocket Sampler Application
    </description>
    <author email="tommynt@outlook.com" href="https://github.com/TommyNT/BAI-App">
        Pawe≈Ç Kalinowski &amp; Duong Nguyen Tuan
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="SplashScreen" value="screen" />
</widget>
