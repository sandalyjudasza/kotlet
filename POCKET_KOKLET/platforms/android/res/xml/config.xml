<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.nativeaudio" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NativeAudio">
        <param name="android-package" value="com.rjfun.cordova.plugin.nativeaudio.NativeAudio" />
    </feature>
    <name>Pocket Sampler</name>
    <description>
        Pocket Sampler Application
    </description>
    <author email="tommynt@outlook.com" href="https://github.com/TommyNT/BAI-App">
        Pawe≈Ç Kalinowski &amp; Duong Nguyen Tuan
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <allow-intent href="market:*" />
    <splash density="port-hdpi" src="res/drawable-port-hdpi/screen.png" />
    <splash density="port-ldpi" src="res/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="res/drawable-port-mdpi/screen.png" />
    <splash density="port-xhdpi" src="res/drawable-port-xhdpi/screen.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="SplashScreen" value="screen" />
</widget>
